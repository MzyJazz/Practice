str1 = input("Please insert the first sentence: ")
str2 = input("Please insert the second sentence: ")

n = len(str1)
i = 0

if str2[:i] == str1[:n] and i <= n:
    i = i+1
    n = n-1
    print("Reversed sentences")
else:
    print("Sentences are different")

while str2[:i] == str1[:n] and i <= n:
    i = i+1
    n = n-1
